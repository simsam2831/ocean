{% extends 'base.html.twig' %}

{% block title %}Ocean Stream{% endblock %}

{% block body %}
<div>
    <table class="table table-hover">
        <tbody>
        {% for row in eventsToView | batch(8) %}
            <tr class="table-active">
                {% for event in row %}
                    <td>{{ event.nameEvent }}</td>
                {% endfor %}
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>
{% endblock %}
